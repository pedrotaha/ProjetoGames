	<queryString>
		<![CDATA[SELECT
     *,
     round(jv.quantidade*q.valoralugar) AS "totalLinha",
     t."descricao" AS descriptionuser
FROM
     "tblocacao" v INNER JOIN "tbjogolocad" jv ON v."idlocacao" = jv."idlocacao"
     INNER JOIN "tbjogos" j ON jv."idjogo" = j."idjogos"
     INNER JOIN "tbquantidade" q ON j."idjogos" = q."idjogo"
     INNER JOIN "tbusuarios" u ON v."idcli" = u."id"
     INNER JOIN "tbtipousuarios" t ON u."idtipo" = t."idtipo"
WHERE
     v.idlocacao = ( SELECT max(idlocacao) FROM "tblocacao" );]]>
	</queryString>
